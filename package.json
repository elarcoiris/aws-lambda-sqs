{
  "name": "awslambdasqs",
  "version": "1.0.0",
  "description": "A Lambda that processes an SQS event",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf .dist dist build",
    "compile": "tsc",
    "build": "npm run compile && cp package.json dist/ && webpack --mode=production",
    "lint": "eslint 'src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "local": "sam local invoke --event events/event.json",
    "prettier:check": "prettier --check 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "keywords": [
    "Lambda",
    "SQS",
    "AWS",
    "CloudFormation",
    "SAM",
    "YAML"
  ],
  "author": "Prue Phillips",
  "license": "",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.10.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.4",
    "webpack": "5.94.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0"
  }
}
